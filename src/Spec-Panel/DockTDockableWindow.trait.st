Trait {
	#name : 'DockTDockableWindow',
	#instVars : [
		'dockPosition'
	],
	#category : 'Spec-Panel-Base',
	#package : 'Spec-Panel',
	#tag : 'Base'
}

{ #category : 'accessing' }
DockTDockableWindow >> dockPosition [

	^ dockPosition
]

{ #category : 'accessing' }
DockTDockableWindow >> dockPosition: aDockPosition [

	dockPosition := aDockPosition
]
