Trait {
	#name : 'DockTDockableWindow',
	#instVars : [
		'dockArea'
	],
	#category : 'Spec-Panel-Base',
	#package : 'Spec-Panel',
	#tag : 'Base'
}

{ #category : 'accessing' }
DockTDockableWindow >> dockArea [

	^ dockArea
]

{ #category : 'accessing' }
